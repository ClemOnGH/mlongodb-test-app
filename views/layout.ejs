<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title><%= title %></title>
        <% if(css) { %>
        <link rel="stylesheet" href="<%= css %>" />
        <% } %>
    </head>
    <body>
        <% body.forEach((el) => {%> <%- include(el.file, {data: el.data}) %> <% }) %>
    </body>
</html>
